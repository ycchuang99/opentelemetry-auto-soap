{
    "name": "ycchuang99/opentelemetry-auto-soap",
    "description": "OpenTelemetry Auto Instrumentation for SOAP",
    "type": "library",
    "keywords": ["opentelemetry", "otel", "open-telemetry", "soap", "instrumentation"],
    "readme": "README.md",
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-opentelemetry": "*",
        "ext-soap": "*",
        "open-telemetry/api": "^1.0",
        "open-telemetry/sem-conv": "^1.30"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.50"
    },
    "autoload": {
        "psr-4": {
            "OpenTelemetry\\Contrib\\Instrumentation\\Soap\\": "src/"
        },
        "files": [
            "_register.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenTelemetry\\Contrib\\Instrumentation\\Soap\\Tests\\": "tests/"
        }
    },
    "config": {
        "lock": false,
        "sort-packages": true,
        "allow-plugins": {
          "php-http/discovery": false
        }
    }
}